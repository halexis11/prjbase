@model prjbase.Models.ProgramacionCentroEducativo

@{
    ViewData["Title"] = "Eliminar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Confirme que desea eliminar el registro.</h3>
<div>
    <h4>Centro Educativo</h4>
    <hr />
    <div asp-validation-summary="All" class="text-danger"></div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdCentroEducativoNavigation.IdDepartamentoNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdCentroEducativoNavigation.IdDepartamentoNavigation.Nombre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdCentroEducativoNavigation.IdMunicipioNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdCentroEducativoNavigation.IdMunicipioNavigation.Nombre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdCentroEducativoNavigation.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdCentroEducativoNavigation.Id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdCentroEducativoNavigation.Nombre)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdCentroEducativoNavigation.Nombre)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdProgramacionNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdProgramacionNavigation.Fecha)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdProgramacionNavigation.IdBodegaNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdProgramacionNavigation.IdBodegaNavigation.Nombre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdProgramacionNavigation.IdFocalizacionNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdProgramacionNavigation.IdFocalizacionNavigation.Nombre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdProgramacionNavigation.IdRemesaNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdProgramacionNavigation.IdRemesaNavigation.Nombre)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index" asp-route-id="@ViewBag.idprogramacion">Regresar a la lista</a>
    </form>
</div>
